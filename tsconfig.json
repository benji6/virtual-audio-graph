{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": false,
    "strict": true,
    "target": "es5"
  },
  "include": ["src/**/*"]
}
